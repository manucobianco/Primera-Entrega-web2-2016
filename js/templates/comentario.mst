<thead>
  <tr>
    <th>Comentarios</th>
    <th>Valoracion</th>
    {{#admin}}
      <th>Borrar</th>
    {{/admin}}
  </tr>
</thead>
<tbody>
{{#coment}}
  <tr>
    <td>{{comentario}}</td>
    <td>{{valoracion}}</td>
    {{#admin}}
      <td id={{id_comentario}} class="borrarComentarios"><a><span class="fa fa-trash"  aria-hidden="true"></span></a></td>
    {{/admin}}
  </tr>
{{/coment}}
</tbody>
